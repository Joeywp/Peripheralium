{
  "schemaVersion": 1,
  "id": "peripheralium",
  "version": "${version}",
  "name": "Peripheralium",
  "description": "A library for all of my peripheral mods, actually",
  "authors": [
    "SirEdvin"
  ],
  "contact": {
    "homepage": "https://example.com/",
    "issues": "https://example.com/",
    "sources": "https://example.com/"
  },
  "license": "MIT",
  "icon": "assets/peripheralium/pack.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "site.siredvin.peripheralium.FabricPeripheralium"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "site.siredvin.peripheralium.FabricPeripheraliumClient"
      }
    ],
    "fabric-datagen": [
      {
        "adapter": "kotlin",
        "value": "site.siredvin.peripheralium.data.FabricDataGenerators"
      }
    ]
  },
  "mixins": [
    "peripheralium.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "fabric-language-kotlin": "*",
    "minecraft": "1.20",
    "java": ">=17",
    "forgeconfigapiport": ">=7.0.0",
    "computercraft": ">=1.105.0"
  }
}
